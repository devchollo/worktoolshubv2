{
  "version": 2,
  "rewrites": [
    { "source": "views/tools/:slug", "destination": "public/views/tools/:slug.html" },
    { "source": "/tools/:slug/:rest*", "destination": "public/views/tools/:slug/:rest*.html" },
    { "source": "/", "destination": "/views/index.html" },
    { "source": "/(.*)", "destination": "/views/$1" }
  ]
}
